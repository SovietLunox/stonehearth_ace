      "mixintypes": {
         "stonehearth:renewable_resource_node": "remove",
         "stonehearth:commands": "override",
         "stonehearth:resource_node": "override",
         "model_variants": "override"
      },
      "model_variants": {
         "default": {
            "models": [
               {
                  "type": "one_of",
