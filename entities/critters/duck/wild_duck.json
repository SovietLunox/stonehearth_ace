      "stonehearth:loot_drops": {
         "entries": {
            "meat": {
               "num_rolls": {
                  "min": 1,
                  "max": 1
               },
               "items": {
                  "jerky": {
                     "uri": "stonehearth_ace:food:duck_meat"
                  }
               }
            },
            "bones": {
               "num_rolls": {
                  "min": 0,
                  "max": 1
               },
               "items": {
                  "jerky": {
                     "uri": "stonehearth_ace:resources:bone:bone_material"
                  }
               }
            },
            "wool": {
               "num_rolls": {
                  "min": 0,
                  "max": 1
               },
               "items": {
                  "pelt": {
